{{ ansible_managed | comment }}

[nginx-{{ nginx_repository_branch }}]
name=NGINX {{ nginx_repository_branch | capitalize }} Repository
baseurl={{ nginx_repository_mirror }}/{{ _nginx_repository_branch }}{{ _nginx_redhat_repository_distribution }}/$releasever/$basearch/
gpgcheck=1
enabled=1
gpgkey={{ nginx_repository_key_url }}
module_hotfixes=true